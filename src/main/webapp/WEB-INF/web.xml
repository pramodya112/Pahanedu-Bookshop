<?xml version="1.0" encoding="UTF-8"?>
<element> 
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginControl</servlet-name>
        <servlet-class>com.example.control.LoginControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginControl</servlet-name>
        <url-pattern>/LoginControl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ManageStaffControl</servlet-name>
        <servlet-class>com.example.control.ManageStaffControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageStaffControl</servlet-name>
        <url-pattern>/manageStaff</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ManageCustomersControl</servlet-name>
        <servlet-class>com.example.control.ManageCustomersControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageCustomersControl</servlet-name>
        <url-pattern>manageCustomers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StaffManageItemsControl</servlet-name>
        <servlet-class>com.example.control.StaffManageItemsControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StaffManageItemsControl</servlet-name>
        <url-pattern>/staffManageItems</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
    </welcome-file-list>
</web-app>
</element>
